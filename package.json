{
  "name": "collabrate",
  "displayName": "CollabRate",
  "description": "This is the README for your extension \"collabrate\". After writing up a brief description, we recommend including the following sections.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onDidChangeActiveTextEditor",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.showCommentSidebar",
        "title": "CollabRate: Add Comment"
      },
      {
        "command": "extension.navigateToComment",
        "title": "Tree View Comment"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceScheme == 'file'",
          "command": "extension.showCommentSidebar",
          "group": "1_modification"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "collabrate",
          "title": "CollabRate",
          "icon": "$(comment)"
        }
      ]
    },
    "views": {
      "collabrate": [
        {
          "type": "webview",
          "id": "collabrate-general",
          "name": "General Comments"
        },
        {
          "id": "collabrate-inline",
          "name": "Inline comments"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "collabrate-inline",
        "contents": "No inline comments found. Create a new comment by highlighting the relevant code lines, right-clicking and choosing \n\"Collabrate\"\n"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "fast-csv": "^5.0.1"
  },
  "directories": {
    "lib": "lib"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
